        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.5",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
